#Plotter para idt.dat

out_src = 'idt.dat'

set xlabel '% Ruido'
set ylabel '% Aciertos'

set title 'Mediciones de precision para IDT'

plot out_src using 1:2 t 'c0.1' w lines, \
  '' using 1:2 t 'c0.2' w lines, \
  '' using 1:3 t 'c0.3' w lines, \
  '' using 1:4 t 'c0.4' w lines, \
  '' using 1:5 t 'c0.5' w lines
